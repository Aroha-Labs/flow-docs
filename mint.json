      "to": "#07C983"
    }
  },
  "background": {
    "style": "gradient"
  },

  "tabs": [
    {
      "name": "Documentation",
      "url": "documentation"
    },
    {
      "name": "Examples",
      "url": "examples"
    },
   
    {
      "name": "Changelog",
      "url": "changelog"
    }
  ],
  "topbarCtaButton": {
    "name": "Get Started",
    "url": "https://console.mira.network"
  },
  "anchors": [
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://discord.com/invite/mira-network"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "documentation/get-started/introduction",
        "documentation/get-started/installation",
        "documentation/get-started/models"
      ]
    },
    {
      "group": "Core Concepts",
      "pages": [
        "documentation/core-concepts/elemental-flows",
        "documentation/core-concepts/compound-flows",
        "documentation/core-concepts/datasets-and-rag"
      ]
    },
    {
      "group": "How To Guides",
      "pages": [
        "documentation/how-to-guides/using-marketplace-flows",
        "documentation/how-to-guides/building-elemental-flows",
        "documentation/how-to-guides/building-compound-flows",
        "documentation/how-to-guides/implementing-rag-capabilities",
        "documentation/how-to-guides/saving-&-customising-marketplace-flows",
        "documentation/how-to-guides/empowering-flows-with-actions"
      ]
    },
    {
      "group": "Examples",
      "pages": [
        "examples/examples",
        "examples/tutorials"
      ]
    },

    {
      "group": "Changelog",
      "pages": [
        "changelog/product-updates"
      ]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/Mira_Network",
    "github": "https://github.com/Aroha-Labs/flow-docs",
    "linkedin": "https://www.linkedin.com/company/mira-aroha-labs"
  },

  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "search": {
    "prompt": "Search documentation..."
