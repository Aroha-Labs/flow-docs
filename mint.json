{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Mira Flows",
  "theme": "venus",
  "logo": {
    "dark": "/logo/logo-on-dark.svg",
    "light": "/logo/logo-on-light.svg"
  },
  "favicon": "/mira_favicon.svg",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "seo": {
    "indexHiddenPages": false
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },
  "feedback": {
    "suggestEdit": true,
    "raiseIssue": true,
    "thumbsRating": true
  },
  "primaryTab": {
    "name": "Get Started"
  },
  "tabs": [
    {
      "name": "Examples",
      "url": "examples"
    }
  ],
  "anchors": [
    {
      "name": "Community",
      "icon": "discourse",
      "url": ""
    }
  ],
  "search": {
    "prompt": "Search Mira Flows docs"
  },
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "get-started/introduction",
        "get-started/installation",
        "get-started/models"
      ]
    },
    {
      "group": "Core Concepts",
      "pages": [
        "core-concepts/elemental-flows",
        "core-concepts/compound-flows",
        "core-concepts/datasets-and-rag",
        "core-concepts/available-models"
      ]
    },
    {
      "group": "How To Guides",
      "pages": [
        "how-to-guides/using-marketplace-flows",
        "how-to-guides/building-elemental-flows",
        "how-to-guides/building-compound-flows",
        "how-to-guides/implementing-rag-capabilities",
        "how-to-guides/saving-&-customising-marketplace-flows"
      ]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/Mira_Network",
