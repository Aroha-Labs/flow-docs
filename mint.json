{
  "$schema": "https://mintlify.com/schema.json",
  "name":"mira",
  "theme": "venus",
  "logo": {
    "dark": "/logo/logo-on-dark.svg",
    "light": "/logo/logo-on-light.svg",
    "href": "https://flow-docs.mira.network/documentation/get-started/introduction"
  },
  "favicon": "/mira_favicon.svg",
  "layout": "sidenav",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373",
